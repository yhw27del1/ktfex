<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
 
<urlrewrite> 
	<!-- 插件链接重写 --> 
	<rule>
		<from>
		     /xsjob-([a-zA-Z_0-9.-]*)-([a-zA-Z_0-9.-]*).html 
		</from>
		<to>
			/enterMatch.action?para1=$1&amp;para2=$2
		</to>
	</rule>
  
</urlrewrite>

